{
  "name": "evaporate-codemod",
  "version": "0.0.1",
  "description": "Fork with an automatic refactor based on an AST. EvaporateJS is a Javascript library for browser to S3 multipart resumable uploads",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rm ./output/* || true",
    "prestart": "npm run clean",
    "start": "node src/index.js",
    "build": "webpack --config webpack.config.build.js",
    "collect-types": "webpack-dev-server --open --config webpack.config.types.js",
    "apply-types": "typewiz-node ./output/Evaporate.ts"
  },
  "author": "Matheus Moreira",
  "license": "MIT",
  "devDependencies": {
    "js-to-ts-converter": "^0.16.3",
    "lebab": "^3.1.0",
    "recast": "^0.18.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "typewiz": "^1.2.4",
    "typewiz-core": "^1.2.4",
    "typewiz-node": "^1.2.4",
    "typewiz-webpack": "^1.2.4",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "prettier": "^1.19.1"
  }
}
